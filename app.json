{
  "expo": {
    "name": "Aniwall",
    "slug": "aniwallpaper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/slap.png",
      "resizeMode": "contain",
      "backgroundColor": "#191E31"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mulacode.aniwallpaper"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#191E31"
      },
      "package": "com.mulacode.aniwallpaper"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "87191d48-a178-4219-abf0-819ea4d47035"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-2514671156039106~5354880712",
    "ios_app_id": "ca-app-pub-xxxxxxxx~xxxxxxxx"
  }
}